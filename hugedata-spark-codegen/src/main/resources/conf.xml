            <property name="enableSubPackages" value="true" />  
            <property name="trimStrings" value="true" />  
            <property name="rootClass" value="cn.com.hugedata.spark.commons.storage.base.BaseEntity"/>
        </javaModelGenerator>  
  
        <sqlMapGenerator 
        	targetPackage="mapper_xml"  
            targetProject="gen">  
            <property name="enableSubPackages" value="true" />  
        </sqlMapGenerator>  
  
		<!--修改targetPackage -->
        <javaClientGenerator type="cn.com.hugedata.spark.codegen.HugedataJavaClientGenerator"
            targetPackage="cn.com.hugedata.spark.commons.storage.mapper" targetProject="gen" >
            <property name="enableSubPackages" value="true" />  
            <property name="rootInterface" value="cn.com.hugedata.spark.commons.storage.base.BaseMapper"/>
            <property name="managementInterface" value="cn.com.hugedata.spark.commons.storage.base.ManagementMapper" />
        </javaClientGenerator>  
  
